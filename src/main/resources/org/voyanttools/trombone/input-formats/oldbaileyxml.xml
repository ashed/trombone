<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="xmlTitleXpath">concat(/div1/interp[@type='year']/@value,' (',/div1/@id, ')')</entry>
    <entry key="xmlPubDateXpath">/div1/interp[@type='year']/@value</entry>
    <entry key="xmlExtraMetadataXpath">Punishment=//interp[@type="punishmentCategory"]/@value
        Verdict=//interp[@type="verdictCategory"]/@value
        Offence=//interp[@type="offenceCategory"]/@value
        People=//persName</entry>
</properties>